{
  "name": "laptop-shop-web-express-v1",
  "version": "1.0.0",
  "description": "laptop shop web express v1",
  "main": "dist/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon",
    "dev": "nodemon",
    "start:debug": "nodemon --inspect=9229 -e ts,tsx --exec node -r ts-node/register ./src/app.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "node_modules"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tranvanhung2003/laptop-shop-web-express-v1.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": {
    "name": "Trần Văn Hưng",
    "url": "https://github.com/tranvanhung2003"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tranvanhung2003/laptop-shop-web-express-v1/issues"
  },
  "homepage": "https://github.com/tranvanhung2003/laptop-shop-web-express-v1#readme",
  "dependencies": {
    "dotenv": "16.4.7",
    "ejs": "3.1.10",
    "express": "5.0.1",
    "mysql2": "3.12.0"
  },
  "devDependencies": {
    "@types/ejs": "3.1.5",
    "@types/express": "5.0.0",
    "@types/node": "22.10.7",
    "nodemon": "3.1.9",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3"
  }
}
